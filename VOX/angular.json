{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "VOX": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "standalone": false
        },
        "@schematics/angular:directive": {
          "standalone": false
        },
        "@schematics/angular:pipe": {
          "standalone": false
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/VOX",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/app/admin/admin-assets"
            ],
            "styles": [
              "src/assets/vendor/css/bundle.min.css",
              "src/assets/vendor/css/jquery.fancybox.min.css",
              "src/assets/vendor/css/owl.carousel.min.css",
              "src/assets/vendor/css/swiper.min.css",
              "src/assets/vendor/css/cubeportfolio.min.css",
              "src/assets/vendor/css/wow.css",
              "src/assets/vendor/css/LineIcons.min.css",
              "src/assets/css/style.css",

              "src/app/admin/admin-assets/assets/css/lib/font-awesome.min.css",
              "src/app/admin/admin-assets/assets/css/lib/themify-icons.css",
              "src/app/admin/admin-assets/assets/css/lib/jsgrid/jsgrid-theme.min.css",
              "src/app/admin/admin-assets/assets/css/lib/jsgrid/jsgrid.min.css",
              "src/app/admin/admin-assets/assets/css/lib/menubar/sidebar.css",
              "src/app/admin/admin-assets/assets/css/lib/bootstrap.min.css",
              "src/app/admin/admin-assets/assets/css/lib/helper.css",
              "src/app/admin/admin-assets/assets/css/style.css",
              "src/app/admin/admin-assets/vendor/owl-carousel/css/owl.carousel.min.css",
              "src/app/admin/admin-assets/vendor/owl-carousel/css/owl.theme.default.min.css",
              "src/app/admin/admin-assets/vendor/jqvmap/css/jqvmap.min.css",
              "src/app/admin/admin-assets/css/style.css"
            ],
            "scripts": [
              "src/assets/vendor/js/bundle.min.js",
              "src/assets/vendor/js/jquery.fancybox.min.js",
              "src/assets/vendor/js/owl.carousel.min.js",
              "src/assets/vendor/js/swiper.min.js",
              "src/assets/vendor/js/jquery.cubeportfolio.min.js",
              "src/assets/vendor/js/parallaxie.min.js",
              "src/assets/vendor/js/wow.min.js",
              "src/assets/js/script.js",
              "src/assets/js/over-laymenu.js",

              "src/app/admin/admin-assets/assets/js/lib/jquery.min.js",
              "src/app/admin/admin-assets/assets/js/lib/jquery.nanoscroller.min.js",
              "src/app/admin/admin-assets/assets/js/lib/menubar/sidebar.js",
              "src/app/admin/admin-assets/assets/js/lib/preloader/pace.min.js",
              "src/app/admin/admin-assets/assets/js/scripts.js",
              "src/app/admin/admin-assets/vendor/global/global.min.js",
              "src/app/admin/admin-assets/js/quixnav-init.js",
              "src/app/admin/admin-assets/js/custom.min.js",
              "src/app/admin/admin-assets/assets/js/lib/jsgrid/db.js",
              "src/app/admin/admin-assets/assets/js/lib/jsgrid/jsgrid.core.js",
              "src/app/admin/admin-assets/assets/js/lib/jsgrid/jsgrid.load-indicator.js",
              "src/app/admin/admin-assets/assets/js/lib/jsgrid/jsgrid.load-strategies.js",
              "src/app/admin/admin-assets/assets/js/lib/jsgrid/jsgrid.sort-strategies.js",
              "src/app/admin/admin-assets/assets/js/lib/jsgrid/jsgrid.field.js",
              "src/app/admin/admin-assets/assets/js/lib/jsgrid/fields/jsgrid.field.text.js",
              "src/app/admin/admin-assets/assets/js/lib/jsgrid/fields/jsgrid.field.number.js",
              "src/app/admin/admin-assets/assets/js/lib/jsgrid/fields/jsgrid.field.select.js",
              "src/app/admin/admin-assets/assets/js/lib/jsgrid/fields/jsgrid.field.checkbox.js",
              "src/app/admin/admin-assets/assets/js/lib/jsgrid/fields/jsgrid.field.control.js",
              "src/app/admin/admin-assets/assets/js/lib/jsgrid/jsgrid-init.js",
              "src/app/admin/admin-assets/vendor/raphael/raphael.min.js",
              "src/app/admin/admin-assets/vendor/morris/morris.min.js",
              "src/app/admin/admin-assets/vendor/circle-progress/circle-progress.min.js",
              "src/app/admin/admin-assets/vendor/chart.js/Chart.bundle.min.js",
              "src/app/admin/admin-assets/vendor/gaugeJS/dist/gauge.min.js",
              "src/app/admin/admin-assets/vendor/flot/jquery.flot.js",
              "src/app/admin/admin-assets/vendor/flot/jquery.flot.resize.js",
              "src/app/admin/admin-assets/vendor/owl-carousel/js/owl.carousel.min.js",
              "src/app/admin/admin-assets/vendor/jqvmap/js/jquery.vmap.min.js",
              "src/app/admin/admin-assets/vendor/jqvmap/js/jquery.vmap.usa.js",
              "src/app/admin/admin-assets/vendor/jquery.counterup/jquery.counterup.min.js",
              "src/app/admin/admin-assets/js/dashboard/dashboard-1.js"
            ],
            "server": "src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "VOX:build:production"
            },
            "development": {
              "buildTarget": "VOX:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "VOX:build"
          }
        },
        "VOX": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "2c0c4a18-2fd4-41dd-a0fd-ab04f73400dd"
  }
}
